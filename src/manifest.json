{
  "name": "Integrity",
  "version": "1.0.0",
  "description": "Analyze any web page",
  "manifest_version": 2,
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "Integrity",
    "default_popup": "index.html"
  }
}
